<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="game.Controller"
          xmlns:fx="http://javafx.com/fxml"
          stylesheets="/game/tictactoe.css"
          alignment="center" hgap="10" vgap="10">
    <Button GridPane.columnIndex="0" GridPane.rowIndex="3" onAction="#clickTile" />
    <Button GridPane.columnIndex="1" GridPane.rowIndex="3" onAction="#clickTile" />
    <Button GridPane.columnIndex="2" GridPane.rowIndex="3" onAction="#clickTile" />
    <Button GridPane.columnIndex="0" GridPane.rowIndex="4" onAction="#clickTile" />
    <Button GridPane.columnIndex="1" GridPane.rowIndex="4" onAction="#clickTile" />
    <Button GridPane.columnIndex="2" GridPane.rowIndex="4" onAction="#clickTile" />
    <Button GridPane.columnIndex="0" GridPane.rowIndex="5" onAction="#clickTile" />
    <Button GridPane.columnIndex="1" GridPane.rowIndex="5" onAction="#clickTile" />
    <Button GridPane.columnIndex="2" GridPane.rowIndex="5" onAction="#clickTile" />
    <Label GridPane.rowIndex="10" GridPane.columnSpan="3" fx:id="testLabel" />
</GridPane>